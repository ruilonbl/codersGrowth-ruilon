{
  "_version": "1.12.0",
  "sap.app": {
    "id": "sap.ui.demo.academia",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "sap.ui.demo.academia.view.App",
      "type": "XML",
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.93",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap.ui.demo.academia.i18n.i18n",
          "supportedLocales": [
            ""
          ],
          "fallbackLocale": ""
        }
      }
    },
    "routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "sap.ui.demo.academia.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "ListaDeAlunos",
					"target": "academia"
				},
				{
					"pattern": "Detalhes/{id}",
					"name": "detalhes",
					"target": "detalhes"
				},
        {
          "pattern": "Cadastro",
					"name": "cadastro",
					"target": "cadastro"
        },
        {
          "pattern": "Editar/{id}",
					"name": "editar",
					"target": "cadastro"
        }
			],
			"targets": {
				"academia": {
					"id": "academia",
					"name": "Academia"
				},
				"detalhes": {
					"id": "detalhes",
					"name": "Detalhes"
				},
          "cadastro": {
            "id": "cadastro",
            "name": "Cadastro"
        }
			}
		}
  }
}